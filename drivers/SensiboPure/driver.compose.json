{
  "id": "SensiboPure",
  "name": {
    "en": "Sensibo Pure"
  },
  "icon": "{{driverAssetsPath}}/icon.svg",
  "images": {
    "large": "{{driverAssetsPath}}/images/large.png",
    "small": "{{driverAssetsPath}}/images/small.png"
  },
  "class": "fan",
  "connectivity": ["cloud"],
  "capabilities": [
    "se_onoff",
    "se_fanlevel",
    "se_last_seen",
    "se_last_seen_seconds"
  ],
  "capabilitiesOptions": {
    "se_onoff": {
      "greyout": true
    }
  },
  "pair": [
    {
      "id": "enter_api_key"
    },
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ],
  "repair": [
    {
      "id": "enter_api_key"
    }
  ]
}
